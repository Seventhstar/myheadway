<%= form_for(@denial) do |f| %>

  <div class="field">
    <%= f.label t :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label t(:start_date) %><br>
    <%= f.text_field :start_date,:value=>@denial.denial_start_date, :class => 'datepicker' %>
  </div>

  <div class="actions">
    <%= submit_cancel denials_path %>
  </div>

<% end %>
